#include <bits/stdc++.h>
using namespace std;

void printArray(vector<int> a) {
    for (int x : a) cout << x << " ";
    cout << endl;
}

////////////////////////////////////////
// 1. Selection Sort
////////////////////////////////////////
void selectionSort(vector<int> &a) {
    int n = a.size();
    for (int i = 0; i < n - 1; i++) {
        int minIndex = i;
        for (int j = i + 1; j < n; j++)
            if (a[j] < a[minIndex])
                minIndex = j;

        swap(a[i], a[minIndex]);
    }
}

////////////////////////////////////////
// 2. Insertion Sort
////////////////////////////////////////
void insertionSort(vector<int> &a) {
    int n = a.size();
    for (int i = 1; i < n; i++) {
        int key = a[i];
        int j = i - 1;

        while (j >= 0 && a[j] > key) {
            a[j + 1] = a[j];
            j--;
        }
        a[j + 1] = key;
    }
}

////////////////////////////////////////
// 3. Bubble Sort
////////////////////////////////////////
void bubbleSort(vector<int> &a) {
    int n = a.size();
    for (int i = 0; i < n - 1; i++)
        for (int j = 0; j < n - i - 1; j++)
            if (a[j] > a[j + 1])
                swap(a[j], a[j + 1]);
}

////////////////////////////////////////
// 4. Merge Sort
////////////////////////////////////////
void mergeArrays(vector<int> &a, int l, int m, int r) {
    vector<int> left(a.begin() + l, a.begin() + m + 1);
    vector<int> right(a.begin() + m + 1, a.begin() + r + 1);

    int i = 0, j = 0, k = l;

    while (i < left.size() && j < right.size()) {
        if (left[i] <= right[j]) a[k++] = left[i++];
        else a[k++] = right[j++];
    }
    while (i < left.size()) a[k++] = left[i++];
    while (j < right.size()) a[k++] = right[j++];
}

void mergeSort(vector<int> &a, int l, int r) {
    if (l < r) {
        int m = (l + r) / 2;
        mergeSort(a, l, m);
        mergeSort(a, m + 1, r);
        mergeArrays(a, l, m, r);
    }
}

////////////////////////////////////////
// 5. Quick Sort
////////////////////////////////////////
int partitionArray(vector<int> &a, int low, int high) {
    int pivot = a[high];
    int i = low - 1;

    for (int j = low; j < high; j++) {
        if (a[j] < pivot) {
            i++;
            swap(a[i], a[j]);
        }
    }

    swap(a[i + 1], a[high]);
    return i + 1;
}

void quickSort(vector<int> &a, int low, int high) {
    if (low < high) {
        int pi = partitionArray(a, low, high);
        quickSort(a, low, pi - 1);
        quickSort(a, pi + 1, high);
    }
}

////////////////////////////////////////
// MAIN FUNCTION
////////////////////////////////////////
int main() {
    vector<int> arr = {64, 25, 12, 22, 11};

    cout << "Original Array: ";
    printArray(arr);

    vector<int> a;

    a = arr;
    selectionSort(a);
    cout << "Selection Sort: ";
    printArray(a);

    a = arr;
    insertionSort(a);
    cout << "Insertion Sort: ";
    printArray(a);

    a = arr;
    bubbleSort(a);
    cout << "Bubble Sort: ";
    printArray(a);

    a = arr;
    mergeSort(a, 0, a.size() - 1);
    cout << "Merge Sort: ";
    printArray(a);

    a = arr;
    quickSort(a, 0, a.size() - 1);
    cout << "Quick Sort: ";
    printArray(a);

    return 0;
}
